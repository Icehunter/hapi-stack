{
    "patterns": [{
        "match": "DEBUG",
        "replacement": false
    }, {
        "match": "SERVERS",
        "replacement": [{
            "key": "api",
            "host": "127.0.0.1",
            "domain": "127.0.0.1",
            "port": 8000,
            "https": false,
            "certificatesPath": "/opt/hapi-stack/certificates",
            "config": {}
        }, {
            "key": "client",
            "host": "127.0.0.1",
            "domain": "127.0.0.1",
            "port": 3000,
            "https": false,
            "certificatesPath": "/opt/hapi-stack/certificates",
            "config": {}
        }, {
            "key": "socketio",
            "host": "127.0.0.1",
            "domain": "127.0.0.1",
            "port": 4000,
            "https": false,
            "certificatesPath": "/opt/hapi-stack/certificates",
            "config": {}
        }]
    }, {
        "match": "PLUGINS",
        "replacement": [
            "logging",
            "cors",
            "security"
        ]
    }]
}
