{
    "patterns": [{
        "match": "API_DOMAIN",
        "replacement": "127.0.0.1"
    }, {
        "match": "API_HOST",
        "replacement": "127.0.0.1"
    }, {
        "match": "API_PORT",
        "replacement": 8000
    }, {
        "match": "API_HTTPS",
        "replacement": false
    }, {
        "match": "API_DEBUG",
        "replacement": true
    }, {
        "match": "CLIENT_DOMAIN",
        "replacement": "127.0.0.1"
    }, {
        "match": "CLIENT_HOST",
        "replacement": "127.0.0.1"
    }, {
        "match": "CLIENT_PORT",
        "replacement": 3000
    }, {
        "match": "CLIENT_HTTPS",
        "replacement": false
    }, {
        "match": "CLIENT_DEBUG",
        "replacement": true
    }]
}
